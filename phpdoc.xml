<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
    configVersion="3"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://www.phpdoc.org"
>
    <title>Qi Db Docs</title>
    <setting name="template.color" value="amber"/>
    <paths>
        <output>docs</output>
        <cache>docs/.cache</cache>
    </paths>
    <version number="latest">
        <api>
            <source dsn=".">
                <path>lib</path>
            </source>
            <ignore hidden="true" symlinks="true">
                <path>tests/**/*</path>
            </ignore>
        </api>
    </version>
</phpdocumentor>
